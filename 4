import java.util.*;
class SpyNo {
    public static void main(String[] args) {
        Scanner scanner=new Scanner(System.in);
        int n=scanner.nextInt();
        int i=n;
        int s=0;
        int p=1;
        while(i!=0)
        {
            s+=i%10;
            p*=i%10;
            i/=10;
        }
       
        if(s==p)
        System.out.println("Spy");
        else
        System.out.println("Not Spy");
    }
}
